{
  "name": "api-node",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "vitest",
    "dev": "tsx --watch src/server.ts",
    "knex": "node --loader tsx ./node_modules/.bin/knex",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "eslint src --ext .tsx,.ts",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsup src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cookie": "^8.3.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.12.0",
    "knex": "^2.4.2",
    "pg": "^8.9.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@rocketseat/eslint-config": "^1.2.0",
    "@types/node": "^18.11.19",
    "@types/supertest": "^2.0.12",
    "commitizen": "^4.3.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "supertest": "^6.3.3",
    "tsup": "^6.6.0",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.28.4",
    "sqlite3": "^5.1.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "src/**": "npm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
